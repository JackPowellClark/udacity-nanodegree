{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": false
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-magic-numbers": "warn"
    }
}